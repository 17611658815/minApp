/**index.wxss**/

.mayfirstBox {
    width: 300rpx;
    height: 300rpx;
    background: yellow;
    position: fixed;
    bottom: -300rpx;
    left: 0;
}

.changeColor {
    width: 300rpx;
    height: 300rpx;
    background: yellow;
    position: fixed;
    bottom: 0;
    left: 0;
    animation: myfirst 0.5s;
}

@keyframes myfirst {
    0% {
        bottom: -300rpx;
    }
