.emergency-shade {
    width: 100%;
    min-height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
}

.emergency-contentHide {
    width: 95%;
    height: 80%;
    display: flex;
    align-items: center;
    flex-direction: column;
    position: fixed;
    left: 2.5%;
    bottom: -80%;
    background: linear-gradient(to bottom, #f9fafe, #eff3ff);
    z-index: 999;
      overflow-y: scroll;
    animation: myfirsts 0.5s;
    /* padding-bottom: env(safe-area-inset-bottom) */
}

.emergency-contentShow {
     width: 95%;
    height: 80%;
    position: fixed;
     left: 2.5%;
    bottom: 0;
    display: flex;
    align-items: center;
    flex-direction: column;
    background: linear-gradient(to bottom, #f9fafe, #eff3ff);
    z-index: 999;
    overflow-y: scroll;
    animation: myfirsts 0.5s;
    /* padding-bottom: env(safe-area-inset-bottom) */
}

@keyframes myfirsts {
    0% {
        bottom: -80%;
    }

    100% {
        bottom: 0;
    }
}

.delImg {
