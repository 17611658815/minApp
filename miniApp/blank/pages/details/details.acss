.cleaningDetails-header {
  width: 100%;
  height: 600rpx;
  /* padding: 80rpx 0; */
  background: white;
  position: relative;
}

.cleaningDetails-header image {
  width: 400rpx;
  height: 400rpx;
}

.swiper {
  height: 600rpx!important;
}

swiper-item {
  width: 100%;
  height: 600rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.layoutBox {
  padding: 10rpx 20rpx;
  background: #F6F6F6;
  position: absolute;
  top: 7%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  align-items: center;
  font-size: 26rpx;
  border-radius: 90rpx;
}

.layoutBox image {
  width: 40rpx;
  height: 40rpx;
  margin: 0 20rpx 0 0;
}

.layoutBox view {
  border-left: 1rpx solid #e2e2e2;
  padding-left: 20rpx;
}

.creditBox {
  padding: 20rpx 0;
  display: flex;
  align-items: center;
  color: #ffffff;
  background-image: linear-gradient(to right, #56CFCA, #E3F4F5);
}

.creditBox image {
  width: 40rpx;
  height: 40rpx;
}

.iconBox {
  width: 50rpx;
  height: 50rpx;
  border: 3rpx solid #ffffff;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 20rpx;
}

.safeguard_img {
  width: 100%;
  height: 300rpx;
}

.leftBtn {
  padding: 10rpx 20rpx;
  border-radius: 90rpx;
  border: 1rpx solid #e2e2e2;
}

.rightBtn {
  padding: 10rpx 20rpx;
  border-radius: 90rpx;
  background: #F8DB41;
}

.nav {
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  background: #f7f7f7;
  white-space: nowrap;
  display: flex;
  box-sizing: border-box;
  background: #ffffff;
}

.nav::-webkit-scrollbar {
  display: none;
}

.nav-item {
  flex-shrink: 0;
  -webkit-box-flex: 0;
  flex-grow: 0;
  text-align: center;
  position: relative;
  font-size: 26rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 0 30rpx;
}

.nav-item.active {
  color: cornflowerblue;
  border-bottom: 2px solid cornflowerblue;
}

.tab-box {
  height: 480px !important;
  background: #fff;
  box-sizing: border-box;
}

.tab-content {
  overflow-y: auto;
}

/* 手机列表 */

