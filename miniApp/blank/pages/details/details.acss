  min-height: 98rpx;
  display: flex;
  flex-direction: column;
  position: fixed;
  background: #ffffff;
  bottom: 0;
  left: 0;
  z-index: 99;
  border-bottom: 2rpx solid #e2e2e2;
}

.addCartBox-box {
  width: 100%;
  min-height: 98rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

button::after {
  border: none;
  border-radius: 0;
}

.addCartBox-box-left {
  flex: 1;
  display: flex;
  align-items: center;
  background: #ffffff;
}

.addCartBox-left {
  color: #666666;
}

.addCartBox-box-left-item {
  width: 100rpx;
  height: 98rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 22rpx;
  color: #666666;
  position: relative;
}

.addCartBox-box-left-item image {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 10rpx;
}

.addCartBox-box-right {
  display: flex;
}

.addCartBox-box-right view {
  width: 300rpx;
  text-align: center;
  line-height: 98rpx;
  font-size: 28rpx;
  color: #000000;
}

