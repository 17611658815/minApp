/* 卡卷样式 */

.bound {
  width: 250rpx;
  height: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  border: 2rpx solid #FD3F3F;
  text-align: center;
  line-height: 40rpx;
  color: #FD3F3F;
  font-weight: 500;
  font-size: 26rpx;
}

.bound::before {
  content: "";
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  display: block;
  background: #fff;
  position: absolute;
  top: 50%;
  left: -20rpx;
  transform: translateY(-50%);
  border-right: 2rpx solid #FD3F3F;
}

.bound::after {
  content: "";
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  display: block;
  background: #fff;
  position: absolute;
  top: 50%;
  right: -20rpx;
  transform: translateY(-50%);
  border-left: 2rpx solid #FD3F3F;
}

.fix-news {
  position: fixed;
  top: 0;
  z-index: 999999
}



.tabs {
  height: 92rpx;
  position: sticky;
  background-color: #fff;
  z-index: 1;
}

.tabs.shadow {
  box-shadow: rgba(153, 153, 153, 0.24) 0 2px 7px;
}

.tabs-bar {
  padding: 0 30rpx;
  display: flex;
  height: 100%;
}

.tabs-bar-tab {
  bottom: 0;
  flex: 1;
  display: flex;
  justify-content: center;
}

.tabs-bar-tab {
  font-size: 32rpx;
}
.tabs-bar-tab-title.active {
  border-bottom: 3px solid #F8DA48;
}

.tabs-bar-tab-title {
  color: #000000;
  padding: 16rpx 0;
  display: flex;
  align-items: center;
  border-bottom: 3px solid transparent;
  box-sizing: border-box;
}
/* 切换样式 */

.list {
  padding: 32rpx;
  flex-shrink: 0;
  box-sizing: border-box;
  width: 100%;
}
.list image{
  width: 100%;
}
.list-item {
  margin-bottom: 35rpx;
}

.list-wrap {
  overflow: hidden;
}

.list-container {
  display: flex;
  width: 100%;
  transition-duration: 500ms;
}


