    from {
        bottom: -100%;
    }

    to {
        bottom: 0;
    }
}

.slidedown {
    animation: slidedown 0.3s ease-out;
}

.slideup {
    animation: slideup 0.3s ease-in;
}

@-webkit-keyframes slidedown {
    from {
        bottom: 0px;
    }

    to {
        bottom: -100%;
    }
}

