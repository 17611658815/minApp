    width: 30rpx;
    height: 30rpx;
    background-size: 30rpx;
    float: left;
}

.box .box_t {
    position: relative;
    margin-top: -110rpx;
    height: 305rpx;
}

.box .box_t .dt {
    width: 236rpx;
    position: absolute;
    bottom: 0;
    left: -10rpx;
}

.box .box_t .dt .dt_img {
    width: 236rpx;
    height: 236rpx;
}

.box .box_t .dt .dt_title {
    font-size: 38rpx;
    color: #5590e3;
    text-align: center;
