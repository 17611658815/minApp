}

.box .ul .li_title {
    color: #999;
    margin-right: 15rpx;
    float: left;
}

.box .ul .li_text {
    color: #555;
    float: left;
    display: block;
    width: 190rpx;
    height: 48rpx;
    overflow: hidden;
}

.box .content {
    font-size: 30rpx;
    line-height: 56rpx;
    border-top: 1px solid #e6ebff;
    margin-top: 30rpx;
    padding-top: 30rpx;
    color: #555;
}

.box .content .c_title {
    color: #5590e3;
    display: block;
    font-weight: bold;
}

.box .content .c_info {
    display: block;
    margin-bottom: 20rpx;
}

.tab_date {
    position: fixed;
    bottom: 0;
    background: url(https://api.xzw.com/static/image/xcx/tab_date.png) no-repeat 0 0;
    background-size: 750rpx 120rpx;
    width: 750rpx;
    height: 120rpx;
    font-size: 36rpx;
    z-index: 55;
    padding: 0 15rpx;
}

.tab_date .tabli {
    width: 120rpx;
    color: #d1d2ff;
    display: inline-block;
    text-align: center;
    line-height: 56rpx;
    margin-top: 46rpx;
}

.tab_date .tabli_on {
    background: #fff;
    height: 56rpx;
    border-radius: 56rpx;
    color: #7f7ff3;
}

.tab_date2 {
    width: 100%;
    position: fixed;
    bottom: -153.47rpx;
}

.more_app,.share {
    color: #6178ec;
    display: block;
    text-align: center;
    padding: 26.39rpx;
    box-sizing: border-box;
    width: 126.39rpx;
    height: 126.39rpx;
    line-height: 1.2em;
    font-size: 29.17rpx;
    background: url(data:image/png;base64,);
    background-size: cover;
}

.more_app {
    float: left;
}

.share {
    float: right;
}

.more_fortune {
    position: fixed;
    bottom: -148rpx;
    left: 50%;
    transform: translateX(-50%);
}

.tap_more {
    position: relative;
    z-index: 0;
    color: #fff;
    text-align: center;
    border-radius: 50%;
    width: 306.94rpx;
    height: 306.94rpx;
    margin: 0 auto;
    padding-top: 33.33rpx;
    box-sizing: border-box;
    background-image: -webkit-linear-gradient(120deg,rgb(93,81,255) 0%,rgb(130,131,255) 66%,rgb(166,181,255) 100%);
    box-shadow: 0px -5px 13px 0px rgba(58,20,179,0.2);
}

.tap_more .icon {
    width: 39.58rpx;
    height: 36.11rpx;
    display: block;
    margin: 0 auto 22.22rpx;
}

.tap_more .text {
    font-size: 38.89rpx;
    font-weight: bold;
}

.tab_items view {
    color: #fff;
    font-size: 30.64rpx;
    text-align: center;
    border-radius: 50%;
    position: absolute;
    top: 50%;
    left: 50%;
    width: 97.92rpx;
    height: 97.92rpx;
    line-height: 97.92rpx;
    margin: -48.96rpx 0 0 -48.96rpx;
    transform: translate(0,0);
    transition: all .3s;
}

.tab_items view:nth-child(1) {
    transition-delay: 0;
}

.tab_items view:nth-child(2) {
    transition-delay: .1s;
}

.tab_items view:nth-child(3) {
    transition-delay: .2s;
}

.tab_items view:nth-child(4) {
    transition-delay: .3s;
}

.tab_items view:nth-child(5) {
    transition-delay: .4s;
}

.tab_items.show view:nth-child(1) {
    transform: translate(-230rpx,-60rpx);
}

.tab_items.show view:nth-child(2) {
    transform: translate(-155rpx,-185rpx);
}

.tab_items.show view:nth-child(3) {
    transform: translate(0,-230rpx);
}

.tab_items.show view:nth-child(4) {
    transform: translate(155rpx,-185rpx);
}

.tab_items.show view:nth-child(5) {
    transform: translate(230rpx,-60rpx);
}

.tab_items .view1 {
    background-image: -webkit-linear-gradient(120deg,rgb(93,81,255) 0%,rgb(130,131,255) 66%,rgb(166,181,255) 100%);
    box-shadow: 0px -5px 13px 0px rgba(58,20,179,0.2);
}

.tab_items .view2 {
    background-image: -webkit-linear-gradient(120deg,rgb(254,134,47) 0%,rgb(255,203,165) 100%);
    opacity: 0.902;
    box-shadow: 1.5px 2.598px 9px 0px rgba(0,0,0,0.15);
}

.tab_items .view3 {
    box-shadow: 0px 5px 13px 0px rgba(0,0,0,0.15);
    opacity: 0.902;
    background-image: -webkit-linear-gradient(-60deg,rgb(165,201,253) 0%,rgb(67,151,255) 100%);
}

.tab_items .view4 {
    background-image: -webkit-linear-gradient(120deg,rgb(48,176,129) 0%,rgb(172,243,203) 100%);
    opacity: 0.902;
    box-shadow: 0px 5px 13px 0px rgba(0,0,0,0.15);
}

.tab_items .view5 {
    background-image: -webkit-linear-gradient(120deg,rgb(74,183,96) 0%,rgb(155,230,170) 100%);
    opacity: 0.902;
    box-shadow: 0px 5px 13px 0px rgba(0,0,0,0.15);
}

.tab_items .view6 {
    background-image: -webkit-linear-gradient(120deg,rgb(252,39,96) 0%,rgb(255,157,184) 100%);
    opacity: 0.902;
    box-shadow: 0px 5px 13px 0px rgba(0,0,0,0.15);
}

